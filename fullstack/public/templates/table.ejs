<html>
    <head>
        <link href="style.css" rel="stylesheet">
    </head>
    <body>
        <table>
            <thead>
                <th>SNo</th>
                <th>Name</th>
                <th>City</th>
            </thead>
            <%
            for(let x in data){
                %>
                <tr>
                    <td><%=data[x].sno%></td>
                    <td><%=data[x].name%></td>
                    <td><%=data[x].city%></td>
                </tr>
                <%}%>
            
        </table>
    </body>
</html>
